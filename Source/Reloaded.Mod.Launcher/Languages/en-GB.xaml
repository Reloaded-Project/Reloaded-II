    <sys:String x:Key="LoaderSettingsEnableAutomaticUpdates">Automatic Updates</sys:String>
    <sys:String x:Key="LoaderSettingsShowConsole">Show Console</sys:String>
    <sys:String x:Key="LoaderSettingsLoadAsynchronously">Load Asynchronously</sys:String>

